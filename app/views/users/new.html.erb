<h2>Sign Up</h2>

<%=form_for User.new, html: { multipart: true } do |f| %>

<%=f.label :email %>

<%=f.text_field :email %>

<%=f.label :password %>

<%=f.password_field :password %>

<%=f.label :password_confirmation %>

<%=f.password_field :password_confirmation %>

<%=f.label :username %>

<%=f.text_field :username %>

<%= f.label :avatar %>
<%= f.file_field :avatar %>

<%=f.submit "Sign Up" %>

<%end%>
