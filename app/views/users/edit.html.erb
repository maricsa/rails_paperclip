<h2>Edit Profile</h2>

<%=form_for @user do |f| %>

<%=f.label :email %>

<%=f.text_field :email %>

<%=f.label :password %>

<%=f.password_field :password %>

<%=f.label :password_confirmation %>

<%=f.password_field :password_confirmation %>

<%=f.label :username %>

<%=f.text_field :username %>

<%=f.submit "Update" %>

<%end%>

