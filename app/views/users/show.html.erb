<div id="user">
<h2>Welcome <%= @user.username %></h2>
<p><%= image_tag @user.avatar.url(:small) %></p>
<p><%= @user.email %></p>
<p><%= @user.username %></p>
</div>

<div id ="cd">
<h3><%= link_to("Delete Profile", user_path(@user), method: :delete) %></h3>
<h3><%= link_to "Edit Profile", edit_user_path(@user) %></h3>
<%= link_to "Go Home", "/" %>
<br>
</div>
